<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.vatvit.irccloudandroid"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application android:icon="@drawable/icon" android:label="IRCCloud" android:name="IRCCloudApplication">
        <activity android:name=".MainActivity" android:label="IRCCloud">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ServersActivity" android:label="Servers">
        </activity>
        <activity android:name="ServerActivity"></activity>
        <activity android:name="ChannelActivity"></activity>

    </application>
</manifest>